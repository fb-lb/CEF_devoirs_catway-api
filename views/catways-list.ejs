<% layout('layout') %>

<main>
    <h1>Liste des catways</h1>
    <p class="error"><%= catways.message %></p>
    <table id="catwaysList">
        <thead>
            <tr>
                <th>Identifiant</th>
                <th>Numéro du catway</th>
                <th>Type du catway</th>
                <th>&Eacutetat du catway</th>
                <th>Détails</th>
            </tr>
        </thead>
        <tbody>
            <% catways.forEach((catway) => { %>
            <tr>
                <td><%= catway._id %></td>
                <td><%= catway.catwayNumber %></td>
                <td><%= catway.type %></td>
                <td><%= catway.catwayState %></td>
                <td><a href="/catway/<%= catway._id%>">Voir la fiche</a></td>
            </tr>
            <% }) %>
        </tbody>
    </table>
</main>